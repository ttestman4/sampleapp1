{
  "name": "spa",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "yarn build non-functional",
    "build:app": "yarn build --base-href /$GITHUB_REPO/ --prod",
    "build:release": "yarn lint && yarn test:ci && yarn e2e --protractor-config=e2e/protractor-ci.conf.js && yarn build:lib && yarn build:app",
    "test": "yarn test:lib && yarn build:lib && yarn test:app",
    "test:ci": "yarn test:lib -i && yarn build:lib && yarn test:app -i",
    "test:lib": "jest --config ./jest.lib.config.js",
    "test:app": "jest --config ./jest.app.config.js",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.3",
    "@angular/common": "~7.1.3",
    "@angular/compiler": "~7.1.3",
    "@angular/core": "~7.1.3",
    "@angular/forms": "~7.1.3",
    "@angular/http": "~7.1.3",
    "@angular/platform-browser": "~7.1.3",
    "@angular/platform-browser-dynamic": "~7.1.3",
    "@angular/router": "~7.1.3",
    "core-js": "^2.5.4",
    "ngx-logger": "^3.3.6",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.1.3",
    "@angular/compiler-cli": "~7.1.3",
    "@angular/language-service": "~7.1.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
